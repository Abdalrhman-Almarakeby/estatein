{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": [
    "^next$",
    "^next/(.*)$",
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/components$",
    "^@/components/(.*)$",
    "^@/containers$",
    "^@/containers/(.*)$",
    "^@/contexts/(.*)$",
    "^@/types$",
    "^@/types/(.*)$",
    "^@/constants$",
    "^@/constants/(.*)$",
    "^@/hooks$",
    "^@/hooks/(.*)$",
    "^@/lib/utils$",
    "^@/lib/(.*)$",
    "^@/(.*)$",
    "^@/assets$",
    "^@/assets/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": false
}
